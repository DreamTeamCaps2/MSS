					<action input="/quanLy_NhomThuoc.jsp" name="nhomThuocForm"
						path="/quan-ly-nhom-thuoc-xoa" scope="request" type="action.QuanLy_NhomThuoc_XoaAction">
						<forward name="xoaNhomThuoc" path="/quan-ly-nhom-thuoc.do" redirect="true" />

								<forward name="suaBenh" path="/quanLy_DanhSachThuoc_SuaBenh.jsp" />
							</action>	
					<action input="/quanLy_DanhSachThuoc_Them.jsp" name="thuocForm"
						path="/them-thuoc" scope="request" type="action.QuanLy_DanhSachThuoc_ThemAction">

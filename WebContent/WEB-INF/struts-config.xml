							<forward name="themBenh" path="/quanLy_DanhSachThuoc_ThemBenh.jsp" />
						</action>
			
			<action input="/quanLy_LoaiThuoc.jsp" name="loaiThuocForm"
				path="/quan-ly-loai-thuoc" scope="request" type="action.QuanLy_LoaiThuocAction">

						<forward name="xoaLoaiThuoc" path="/quan-ly-loai-thuoc.do" redirect="true" />
					</action>
			<action input="/quanLy_NhomThuoc.jsp" name="nhomThuocForm"
				path="/quan-ly-nhom-thuoc" scope="request" type="action.QuanLy_NhomThuocAction">
